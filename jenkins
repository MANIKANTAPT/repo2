pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building23...'
                sh "mvn Build"
            }
        }

        stage('Test') {
            steps {
                echo "Testing..."
                sh 'mvn Test'
            }
        }

        stage('package') {
            steps {
                echo 'package'
                sh "mvn package"
            }
        }
    }
}


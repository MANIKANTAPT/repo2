pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building23...'
                sh 'mvn Build'
            }
        }

        stage('Test') {
            steps {
                echo 'Testing...'
                sh 'mvn Test'
            }
        }

        stage('Deploy') {
            steps {
                echo 'Deploying'
                sh 'mvn deploy'
            }
        }
    }
}

